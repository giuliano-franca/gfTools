global proc AEgfUtilDoubleToAngleTemplate(string $nodeName){
    editorTemplate -beginScrollLayout;
        editorTemplate -beginLayout "gfUtilDoubleToAngle Attributes" -collapse 0;
            editorTemplate -addControl "double";
        editorTemplate -endLayout;
        AEdependNodeTemplate $nodeName;
    editorTemplate -addExtraControls;
    editorTemplate -endScrollLayout;
}