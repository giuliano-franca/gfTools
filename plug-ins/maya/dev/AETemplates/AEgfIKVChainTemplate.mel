global proc AEgfIKVChainTemplate(string $nodeName){
    editorTemplate -beginScrollLayout;
        editorTemplate -beginLayout "gfIKVChain Attributes" -collapse 0;
            editorTemplate -beginLayout "Input Informations" -collapse 1;
                editorTemplate -addControl "root";
                editorTemplate -addControl "handle";
                editorTemplate -addControl "poleVector";
                editorTemplate -addControl "snap";
                editorTemplate -addControl "parentInverseMatrix";
                editorTemplate -addControl "offset";
                editorTemplate -addControl "jointOrient";
            editorTemplate -endLayout;
            editorTemplate -beginLayout "Inverse Kinematics Options" -collapse 0;
                editorTemplate -addControl "restLengthStart";
                editorTemplate -addControl "restLengthEnd";
                editorTemplate -addControl "compressionLimit";
                editorTemplate -addControl "softness";
                editorTemplate -addControl "snapUpVector";
                editorTemplate -addControl "hierarchyMode";
                editorTemplate -addSeparator;
                editorTemplate -addControl "pvMode";
                editorTemplate -addControl "preferredAngle";
                editorTemplate -addControl "twist";
            editorTemplate -endLayout;
            editorTemplate -beginLayout "Cartoon Options" -collapse 1;
                editorTemplate -addControl "stretch";
                editorTemplate -addControl "useStretchAsScale";
                editorTemplate -addControl "clampStretch";
                editorTemplate -addControl "clampValue";
                editorTemplate -addControl "squash";
                editorTemplate -addControl "squashMultStart";
                editorTemplate -addControl "squashMultEnd";
            editorTemplate -endLayout;
        editorTemplate -endLayout;
        AEdependNodeTemplate $nodeName;
    editorTemplate -addExtraControls;
    editorTemplate -endScrollLayout;
}